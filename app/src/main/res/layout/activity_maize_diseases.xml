<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.hp.kilimo_bora.MaizeDiseases">
    <TextView
        android:layout_width="fill_parent"
        android:id="@+id/textviewmaize"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:textStyle="bold"
        android:text="@string/Maize_diseases"/>

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/MaizeListview"
        android:layout_below="@+id/textviewmaize"
        android:entries="@array/Maize_disease_arrays"
        android:prompt="@string/Maize_diseases">

    </ListView>



</RelativeLayout>
